{
  "name": "waitstaff-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "cross-env NODE_ENV=test echo 'Simulating Test...' && sleep 10 && echo 'All Tests Passed...' && exit 0",
    "nyc": "nyc --reporter=html --reporter=text mocha -r esm --cache=false --timeout=15000 --recursive --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "log:clear": "rm src/logs/app*.log",
    "build:clear": "rm -rf build coverage .nyc_output",
    "compile": "babel src -d build --copy-files",
    "build": "run-s build:clear compile",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "lint": "eslint src --fix",
    "create:JWTKeys": "openssl genrsa -out .private.pem 4096 && openssl rsa -in .private.pem -pubout > .public.pem"
  },
  "author": "Mitchell Patrick",
  "license": "MIT",
